{
  "name": "itstime-be",
  "version": "1.0.0",
  "description": "test node and mongo backend for React Native",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "nodemon ./src --source-maps --out-dir dist",
    "start": "nodemon -- node_modules/babel-cli/bin/babel-node.js src/server.js",
    "populate": "node_modules/babel-cli/bin/babel-node.js src/populate.js"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.3.0",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3"
  },
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "moment": "^2.20.1",
    "mongoose": "^5.0.0-rc0",
    "morgan": "^1.9.0",
    "prettier": "^1.12.0"
  }
}
